void loop()
{

  if (irrecv.decode(&resultados)){
  if (resultados.value == 0XFFFFFFFF)
    resultados.value = key_value;
    
    switch(resultados.value){      
      case 0xFF30CF:
        Serial.println("Botão 1");
        eye_look_right();
        open_eyes(100);
        break;
      
      case 0xFF18E7:
        Serial.println("Botão 2");
        cute_eyes();
        open_eyes(100);
        break;
      
      case 0xFF7A85:
        Serial.println("Botão 3");
        eye_look_left();
        open_eyes(100);
        break;
  }
    key_value = resultados.value;
    irrecv.resume(); 
  }
  
  


